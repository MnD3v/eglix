# Configuration Render pour Migration Forcée Super Admin

# Variables d'environnement critiques
APP_ENV=production
APP_DEBUG=false
LOG_LEVEL=error

# Configuration de la base de données
# DATABASE_URL sera fournie par Render automatiquement

# Configuration du cache
CACHE_DRIVER=file
SESSION_DRIVER=file
QUEUE_CONNECTION=sync

# Configuration de sécurité
APP_KEY=base64:your-app-key-here
SESSION_SECURE_COOKIE=true
SESSION_HTTP_ONLY=true

# Configuration super admin
SUPER_ADMIN_EMAIL=admin@eglix.com
SUPER_ADMIN_PASSWORD=admin123!

# Script de déploiement
BUILD_COMMAND=./script/render-deploy-with-admin-migration.sh
START_COMMAND=php artisan serve --host=0.0.0.0 --port=$PORT

# Configuration PHP
PHP_VERSION=8.2
COMPOSER_VERSION=2

# Configuration des logs
LOG_CHANNEL=stderr
LOG_DEPRECATIONS_CHANNEL=null